issn = {07308078},
journal = {Arxiv preprint arXiv},
number = {November 2011},
pages = {1--17},
pmid = {20840727},
title = {{The Anatomy of the Facebook Social Graph}},
url = {http://arxiv.org/abs/1111.4503v1},
volume = {abs/1111.4},
year = {2011}
}

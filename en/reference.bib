file = {:Users/SunJc/Documents/Mendeley Desktop/Zhou, L{\"{u}}, Li/New Journal of Physics/Zhou, L{\"{u}}, Li - 2012 - Quantifying the influence of scientists and their publications Distinguishing between prestige and popularity.pdf:pdf},

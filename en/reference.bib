@article{Baeza-Yates2006,
abstract = {This paper introduces a family of link-based ranking algorithms that$\backslash$npropagate page importance through links. In these algorithms there$\backslash$nis a damping function that decreases with distance, so a direct link$\backslash$nimplies more endorsement than a link through a long path. PageRank$\backslash$nis the most widely known ranking function of this family. The main$\backslash$nobjective of this paper is to determine whether this family of ranking$\backslash$ntechniques has some interest per se, and how different choices for$\backslash$nthe damping function impact on rank quality and on convergence speed.$\backslash$nEven though our results suggest that PageRank can be approximated$\backslash$nwith other simpler forms of rankings that may be computed more efficiently,$\backslash$nour focus is of more speculative nature, in that it aims at separating$\backslash$nthe kernel of PageRank, that is, link-based importance propagation,$\backslash$nfrom the way propagation decays over paths. We focus on three damping$\backslash$nfunctions, having linear, exponential, and hyperbolic decay on the$\backslash$nlengths of the paths. The exponential decay corresponds to PageRank,$\backslash$nand the other functions are new. Our presentation includes algorithms,$\backslash$nanalysis, comparisons and experiments that study their behavior under$\backslash$ndifferent parameters in real Web graph data. Among other results,$\backslash$nwe show how to calculate a linear approximation that induces a page$\backslash$nordering that is almost identical to PageRank's using a fixed small$\backslash$nnumber of iterations; comparisons were performed using Kendall's$\backslash$ntau on large domain datasets.},
author = {Baeza-Yates, R and Boldi, P and Castillo, C},
doi = {10.1145/1148170.1148225},
file = {:Users/SunJc/Documents/Mendeley Desktop/Baeza-Yates, Boldi, Castillo/Proceedings of the 29th annual international ACM SIGIR conference on Research and development in information retrieval/Baeza-Yates, Boldi, Castillo - 2006 - Generalizing pagerank Damping functions for link-based ranking algorithms.pdf:pdf},
isbn = {1595933697},
journal = {Proceedings of the 29th annual international ACM SIGIR conference on Research and development in information retrieval},
keywords = {link analysis,link-based ranking,web graphs},
pages = {308--315},
title = {{Generalizing pagerank: Damping functions for link-based ranking algorithms}},
year = {2006}
}

